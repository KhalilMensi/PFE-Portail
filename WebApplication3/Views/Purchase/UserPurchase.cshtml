@{
    ViewData["Title"] = "UserPurchase";
}
<div class="container-fluid p-4">
    <div class="row col-md-12 mt-5 mb-5">
        <span style="font-family: 'Roboto Condensed', sans-serif;font-size:35px;" id="NbArt">Vos Commandes (@ViewBag.Count)</span>
    </div>
    <table id="myPurchases" class="table table-striped table-hover" style="background-color:#ffffff;cursor:pointer">
        <thead>
            <tr>
                <th>Numéro du commande</th>
                <th>Date du commande</th>
                <th>Prix HT</th>
                <th>Prix TTC</th>
                <th>Etat du commande</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
@section Scripts{
    <script src="~/assets/js/Commande.js" asp-append-version="true"></script>
    <partial name="_ValidationScriptsPartial" />
}