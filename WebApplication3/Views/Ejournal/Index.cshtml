@{
    ViewData["Title"] = "Index";
}

<div class="container-fluid" style="margin-bottom:80px;">
    <div class="row" style="padding-bottom:1%">
        <div class="col-md-9">
            <h3 style="color:#6777ef">Ejournal List</h3>
        </div>
        <div class="col-md-3">
            <a asp-action="UpsertEjournal" asp-controller="Ejournal" class="btn btn-primary form-control text-white">
                Ajouter Ejournal
            </a>
        </div>
    </div>
    <table id="myTable" class="table table-striped" style="background-color:#ffffff">
        <thead>
            <tr>
                <th>ISSN</th>
                <th>Doi</th>
                <th>Titre</th>
                <th>Langue</th>
                <th>editeur</th>
                <th class="text-center">Actions</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/assets/js/EjournalList.js" asp-append-version="true"></script>
    <partial name="_ValidationScriptsPartial" />
}


